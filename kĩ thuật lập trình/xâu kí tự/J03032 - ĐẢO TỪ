package example;
import java.util.*;

public class test {
    public static Scanner sc = new Scanner(System.in);
    public static void main(String[] args) {
        int t = sc.nextInt();
        sc.nextLine();
        while(t-- > 0){
            String a[] = sc.nextLine().split("\\s+");
            for(String x : a){
                StringBuilder sb = new StringBuilder(x);
                System.out.print(sb.reverse() + " ");
            }
            System.out.println("");
        }
    }
}
