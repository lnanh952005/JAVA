package example;
import java.util.*;

public class test {
    public static Scanner n = new Scanner(System.in);

    public static void main(String[] args) throws Exception {
        float a = n.nextFloat();
        float b = n.nextFloat();
        if(a == 0 && b == 0) System.out.println("VSN");
        else if(a == 0) System.out.println("VN");
        else{
            System.out.printf("%.2f",-b/a);
        }
    }
}
